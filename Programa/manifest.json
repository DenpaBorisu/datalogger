{
  "manifest_version": 2,
  "name": "Arduino Data Logger",
  "description": "Representa graficamente datos recibidos mediante el puerto serial",
  "version": "0.6",
  "author": "Boris Gonz√°lez, PUCV",
  "app": {
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  },
  "frame":false,

  "permissions": [
      "serial",
      "fullscreen"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}
